{
    "name": "terminal42/contao-easy_themes",
    "type": "contao-module",
    "description": "Contao Open Source CMS extension easy_themes",
    "keywords": ["contao", "easy", "themes"],
    "license": "LGPL",
    "authors": [
        {
            "name":"Yanick Witschi",
            "email":"yanick.witschi@terminal42.ch",
            "homepage":"http://www.terminal42.ch",
            "role":"Developer"
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "contao/core": ">=3.2,<4",
        "contao-community-alliance/composer-plugin": "2.*"
    },
    "autoload": {
        "classmap": [""]
    },
    "replace": {
        "contao-legacy/easy_themes": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev",
            "dev-develop": "2.2.x-dev"
        },
        "contao": {
            "sources": {
                "": "system/modules/easy_themes"
            },
            "runonce": [
                "runonce/runonce.php"
            ]
        }
    }
}
